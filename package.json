{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "yabai",
  "title": "Yabai",
  "description": "Keyboard shortcuts for yabai",
  "icon": "command-icon.png",
  "author": "SpicyRicecaker",
  "categories": [
    "Productivity",
    "Developer Tools"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "maximize",
      "title": "Maximize",
      "subtitle": "Yabai",
      "description": "Maximize a window",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.36.1",
    "react": "17.0.2",
    "react-reconciler": "0.26.2"
  },
  "devDependencies": {
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}